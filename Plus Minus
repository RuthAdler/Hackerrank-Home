def plusMinus(arr):
    n = (len(arr)*1.00000)
    positive = 0
    negative = 0
    zero = 0
    for element in arr:
        if element > 0:
            positive +=1
        if element < 0:
            negative +=1
        if element == 0:
            zero +=1    
            
    print(positive/n)
    print(negative/n)
    print(zero/n)
    
